{
  "name": "qavalidator-ui",
  "version": "0.1.0",
  "description": "QAvalidator UI",
  "scripts": {
    "initialize": "npm install --no-optional",
    "production": "webpack --config webpack/webpack.production.config.js --progress --colors --env production",
    "prestart": "open-url http://localhost:3000/index.html",
    "start": "webpack-dev-server --config webpack/webpack.development.config.js --progress --colors --hot --port 3000 --content-base src",
    "postinstall": "typings install"
  },
  "dependencies": {
    "@angular/common": "2.1.2",
    "@angular/compiler": "2.1.2",
    "@angular/compiler-cli": "2.1.2",
    "@angular/core": "2.1.2",
    "@angular/forms": "2.1.2",
    "@angular/http": "2.1.2",
    "@angular/platform-browser": "2.1.2",
    "@angular/platform-browser-dynamic": "2.1.2",
    "@angular/router": "3.1.2",
    "@angular/upgrade": "2.1.2",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.10"
  },
  "devDependencies": {
    "chunk-manifest-webpack-plugin": "0.0.1",
    "clean-webpack-plugin": "0.1.3",
    "html-webpack-plugin": "1.6.2",
    "copy-webpack-plugin": "4.0.0",
    "open-url": "2.0.2",
    "rimraf": "2.4.4",
    "ts-loader": "1.0.0",
    "tslint-loader": "1.0.2",
    "typings": "^1.5.0",
    "typescript": "^2.0.2",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.12.1",
    "webpack-md5-hash": "0.0.4",
    "yargs": "6.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qaware/QAvalidator"
  },
  "private": true
}
